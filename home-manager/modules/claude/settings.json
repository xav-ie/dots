{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "attribution": {
    "commit": "",
    "pr": ""
  },
  "model": "opus",
  "hooks": {
    "Notification": [
      {
        "matcher": "permission_prompt",
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/notify.nu"
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "~/.claude/statusline.nu",
    "padding": 0
  },
  "enabledPlugins": {
    "dts@outsmartly-plugins": true,
    "pts@outsmartly-plugins": true,
    "mgrep@Mixedbread-Grep": true,
    "vtsls@claude-code-lsps": true,
    "vscode-langservers@claude-code-lsps": true,
    "rust-analyzer@claude-code-lsps": true,
    "gopls@claude-code-lsps": true,
    "typescript-lsp@claude-plugins-official": true,
    "beads@beads-marketplace": true,
    "meta-cc@meta-cc-marketplace": true,
    "plugin-dev@claude-plugins-official": true
  },
  "alwaysThinkingEnabled": true
}
